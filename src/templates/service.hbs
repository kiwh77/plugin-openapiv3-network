import { useService } from 'wuhao-network'
import { 
{{#each components}}
  {{this.name}},
{{/each}}
} from './entity'

{{#each paths}}
/**
 * {{this.description}}
{{#each this.responses}}
 * @return {{@key}} {{this.description}}
{{/each}}
 */
export const {{this.name}} = useService([
  '{{this.name}}', 
  '{{this.method}}', 
  '{{this.url}}',
  {
    tags: [{{this.tags}}]
  }
])

{{/each}}